#include <behaviors.dtsi>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/outputs.h>
#include <dt-bindings/zmk/pointing.h>

// define layers
#define COLEMAK 0
#define SYM 1
#define SYS 2
#define NAV 3
#define NUM 4
#define SYS_ADMIN 5

// Helper macros for cleaner keymap definitions
#define ___ &trans
#define XXX &none

// Home row mods macros
#define HML(key, mod) &ht mod key
#define HMR(key, mod) &ht mod key

// Layer tap macro (named LTap to avoid conflict with ZMK's LT key)
#define LTap(layer, key) &lt layer key

// Momentary layer macro
#define MO(layer) &mo layer

// Shift hold-tap macros (RSH to avoid conflict with ZMK's RSHFT key)
#define SHFT(key) &ht LSHFT key
#define RSH(key) &ht RSHFT key

// Mouse movement macros
#define MV_U &mmv MOVE_UP
#define MV_D &mmv MOVE_DOWN
#define MV_L &mmv MOVE_LEFT
#define MV_R &mmv MOVE_RIGHT

// Mouse scroll macros
#define SC_U &msc MOVE_UP
#define SC_D &msc MOVE_DOWN
#define SC_L &msc MOVE_LEFT
#define SC_R &msc MOVE_RIGHT

// Mouse button macros
#define MCLICK1 &mkp MB1
#define MCLICK2 &mkp MB2

// Media control macros
#define PREV &kp C_PREV
#define PLAY &kp C_PP
#define NEXT &kp C_NEXT
#define VOLD &kp C_VOL_DN
#define VOLU &kp C_VOL_UP
#define MUTE &kp C_MUTE

// Bluetooth macros
#define BT0 &bt BT_SEL 0
#define BT1 &bt BT_SEL 1
#define BT2 &bt BT_SEL 2
#define BT3 &bt BT_SEL 3
#define BT_DISC0 &bt BT_DISC 0
#define BT_DISC1 &bt BT_DISC 1
#define BT_DISC2 &bt BT_DISC 2
#define BTCLR &bt BT_CLR
#define BTCLRA &bt BT_CLR_ALL

// System macros
#define BOOT &bootloader
#define OUTTOG &out OUT_TOG

// Configure the layer tap.
&lt {
    // Setting quick-tap-ms will allow me to tap then hold the backspace key
    // and have it repeat.
    quick-tap-ms = <200>;
};

/ {
    behaviors {
        ht: hold_tap {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            flavor = "tap-preferred";
            tapping-term-ms = <200>;
            quick-tap-ms = <200>;
            #require-prior-idle-ms = <125>;
            bindings = <&kp>, <&kp>;
        };
    };

    conditional_layers {
        compatible = "zmk,conditional-layers";

        sys_admin_left {
            if-layers = <SYM SYS>;
            then-layer = <SYS_ADMIN>;
        };

        sys_admin_right {
            if-layers = <NAV NUM>;
            then-layer = <SYS_ADMIN>;
        };
    };

    keymap {
        compatible = "zmk,keymap";

